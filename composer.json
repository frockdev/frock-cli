{
    "name": "frockdev/frock-cli",
    "description": "Toolset for development with k8s.",
    "homepage": "https://frock.dev",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/frockdev/frock-cli/issues",
        "source": "https://github.com/frockdev/frock-cli"
    },
    "authors": [
        {
            "name": "Vladimir Zakotnev",
            "email": "vladitot@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "czproject/git-php": "^4.2",
        "laravel-zero/phar-updater": "^1.3",
        "nunomaduro/termwind": "^1.15.1",
        "symfony/process": "^6.4",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.22",
        "laravel-zero/framework": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/frock"]
}
